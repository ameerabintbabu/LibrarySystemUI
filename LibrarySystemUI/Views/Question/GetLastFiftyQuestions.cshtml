            <li>
                <label>Question title :</label> <span id="spn_Title"></span>
            </li>
            <li>
                <label>Question Link :</label> <span id="spn_Link"></span>
            </li>
            <li>
                <label>User Id :</label> <span id="spn_user_id"></span>
            </li>

            <li>
                <label>Owner :</label> <span id="spn_display_name"></span>
            </li>
            <li>
                <label>User Type :</label> <span id="spn_user_type"></span>
            </li>

            <li>
                <label>Answer Count :</label> <span id="spn_answer_count"></span>
            </li>
            <li>
                <label>View Count :</label> <span id="spn_view_count"></span>
            </li>

        </ul>
    </div>
    <div id="QuestionDetails" class="settingsList">

    </div>
</div>

