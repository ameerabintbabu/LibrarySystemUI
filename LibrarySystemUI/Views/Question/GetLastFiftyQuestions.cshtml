@model LibrarySystemUI.Models.QuestionInfo

@{
    ViewBag.Title = "GetLastFiftyQuestions";
}
<div id="div_questions">
    <h1>These are the last fifty questions from Stack Overflow :</h1>
       <div id="QuestionList" class="settingsList"></div>
</div>
<div id="div_details">
    </br>
    </br>
    <button type="button" class="btn btn-default btn-success" id="btn_GetQuestions" data-dismiss="modal" onclick="objQuestion.GetLastfiftyQuestions()">Get Last 50 questions from stack overflow</button>
    </br>
    </br>
    <div class="col-xs-6">
        <h3>Details</h3>
        <ul class="styledListing">
            <li>
                <label>QuestionId :</label> <span id="spn_QuestionId"></span>
